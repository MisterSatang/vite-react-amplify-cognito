generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  userId    String  @id
  citizenId String? @unique @db.VarChar(100)
  email     String  @unique
}

model person {
  perosnId  String  @id @default(uuid())
  citizenId String? @unique @db.VarChar(100)
  name      String? @db.VarChar(100)
}
